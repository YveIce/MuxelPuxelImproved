{
  "schemaVersion": 1,
  "id": "mpi",
  "version": "${version}",
  "name": "Muxelpuxel Improved",
  "description": "Replaces the scoreboard on mixelpixel.net in the most cases with a beautiful and simple bar on the top of the screen. ",
  "authors": ["Yve", "Enrico Messall"],
  "contact": {},
  "license": "MIT",
  "icon": "assets/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "click.isreal.mpi.client.mpiClient"
    ],
    "main": [
      "click.isreal.mpi.Mpi"
    ],
    "modmenu": [
      "click.isreal.mpi.ModMenuApiImplementation"
    ]
  },
  "mixins": [
    "topbar.mixins.json"
  ],
  "depends": {
    "cloth-config2": ">=11.1.106",
    "fabricloader": ">=0.14.22",
    "fabric": ">=0.89.0",
    "minecraft": ">=1.20.1"
  }
}
